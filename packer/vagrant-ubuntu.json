{
  "builders": [
    {
      "type": "vagrant",
      "communicator": "ssh",
      "source_path": "https://cloud-images.ubuntu.com/bionic/current/bionic-server-cloudimg-amd64-vagrant.box",
      "provider": "virtualbox",
      "add_force": true,
      "insert_key": true,
      "box_name": "petclinic-ubuntu-18.04"
    }
  ],
  "provisioners": [{
    "type": "shell",
    "inline": ["echo foo"]
  },{
    "type": "ansible",
    "playbook_file": "./ansible/petclinic-docker-roles.yml",
    "extra_arguments": [
        "--become"
    ]
  }]
}